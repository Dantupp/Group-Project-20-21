{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "videoplayer",
    "type": "object",
    "properties": {
      "userid": {
        "description": "Takes user's ID",
        "type": "string"
      },
      "pageid": {
        "description": "Takes page's ID",
        "type": "string"
      },
      "VideoID": {
        "description": "Takes a video hyperlink on the given page",
        "type": "string"
      },
      "Timewatched": {
        "description": "How much of the video was watched",
        "type": "integer"
      }
    },
    "required": [
      "userid",
      "pageid",
      "VideoID",
      "Timewatched"
    ]
  }