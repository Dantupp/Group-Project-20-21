{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "userinput",
    "type": "object",
    "properties": {
      "userid": {
          "description": "Takes user's ID",
        "type": "string"
      },
      "pageid": {
          "description": "Takes page's ID",
        "type": "string"
      },
      "inputID": {
          "description": "Takes the ID of the input on given page",
        "type": "string"
      },
      "Time": {
          "description": "Time taken until the user interacted with that UI",
        "type": "integer"
      }
    },
    "required": [
      "userid",
      "pageid",
      "inputID",
      "Time"
    ]
  }